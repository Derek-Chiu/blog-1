<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techapp.james.service">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".intentService.IntentServiceActivity" />

        <service
            android:name=".intentService.MyIntentService"
            android:exported="false" />

        <activity android:name=".normalService.NormalServiceActivity" />

        <service
            android:name=".normalService.MyService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".bindService.BindServiceActivity" />

        <service
            android:name=".bindService.MyBindService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".jobSheduler.JobSchedulerActivity"></activity>
    </application>

</manifest>